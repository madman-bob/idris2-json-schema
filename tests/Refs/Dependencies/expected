1/1: Building BasicDependencies (BasicDependencies.idr)
public export
Title : Type
Title = String

public export
Year : Type
Year = Double

public export
record Film where
    constructor MkFilm
    title : Main.Title
    year : Maybe Main.Year

public export
Main : Type
Main = List Main.Film
---
1/1: Building CircularDependencies (CircularDependencies.idr)
mutual
    public export
    data Even = Even_0 () | Even_1 Main.Odd

    public export
    Odd : Type
    Odd = Main.Even

public export
Main : Type
Main = Main.Even
